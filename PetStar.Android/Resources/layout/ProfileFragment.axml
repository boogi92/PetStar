                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:textColor="#ff808080"
                            android:gravity="center" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="80.0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout5">
                        <Button
                            android:text="+"
                            android:id="@+id/button1"
                            android:background="@mipmap/btnplus"
                            android:textColor="#ff818181"
                            android:layout_width="80.0dp"
                            android:layout_height="80.0dp" />
                        <TextView
                            android:text="Text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:textColor="#ff808080"
                            android:gravity="center" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="80.0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout5">
                        <Button
                            android:text="+"
                            android:id="@+id/button1"
                            android:background="@mipmap/btnplus"
                            android:textColor="#ff818181"
                            android:layout_width="80.0dp"
                            android:layout_height="80.0dp" />
                        <TextView
                            android:text="Text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:textColor="#ff808080"
                            android:gravity="center" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="80.0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout5">
                        <Button
                            android:text="+"
                            android:id="@+id/button1"
                            android:background="@mipmap/btnplus"
                            android:textColor="#ff818181"
                            android:layout_width="80.0dp"
                            android:layout_height="80.0dp" />
                        <TextView
                            android:text="Text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:textColor="#ff808080"
                            android:gravity="center" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="80.0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout5">
                        <Button
                            android:text="+"
                            android:id="@+id/button1"
                            android:background="@mipmap/btnplus"
                            android:textColor="#ff818181"
                            android:layout_width="80.0dp"
                            android:layout_height="80.0dp" />
                        <TextView
                            android:text="Text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView2"
                            android:textColor="#ff808080"
                            android:gravity="center" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="80.0dp"
